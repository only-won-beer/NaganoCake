
<containar>
  <div class="row justify-content-center">
    <div class="col-md-10 offset-md-2">
      <h4>商品一覧</h4>
      <table>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
        <th>ジャンル</th>
        <th>ステータス</th>

        <% @items.each do |item| %>
          <tr>
            <td><%= item.id %></td>
            <td><%= link_to admin_item_path(item.id) do %>
                  <%= item.name %>
                <% end %></td>
            <td><%= item.price %></td>
            <td><%= item.genre_id %></td>
            <td><%= item.is_active %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</containar>

